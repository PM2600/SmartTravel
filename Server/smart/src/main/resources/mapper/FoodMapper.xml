<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.mapper.FoodMapper">

    <select id="getFood" resultType="com.app.entity.Food">
        SELECT
            uid, foodtext, foodlike
        FROM
            t_food
    </select>



</mapper>